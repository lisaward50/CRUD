<% include ./partials/header %>

<div class="container">
  <br/>
  <h1>Add a New Whisky</h1>
  <br/>
  <form action="/whiskies" method="POST">
    <input type="text" name="whisky[name]" placeholder="name">
    <input type="text" name="whisky[image]" placeholder="image url">
    <input type="text" name="whisky[type]" placeholder="type">
    <input type="text" name="whisky[description]" placeholder="description">
    <button class="btn btn-sm btn-outline-info">Submit</button>
  </form>
  <br/>
  <a class="btn btn-sm btn-outline-primary" href="/whiskies">Return to list</a>
</div>

<% include ./partials/footer %>
